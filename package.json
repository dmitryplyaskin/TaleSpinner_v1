{
  "name": "TaleSpinner",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "install:server": "yarn --cwd server install",
    "install:web": "yarn --cwd web install",
    "install:docs": "yarn --cwd docs install",
    "install:all": "run-s install:server install:web",
    "dev:server": "yarn --cwd server dev",
    "dev:web": "yarn --cwd web dev",
    "dev": "run-p dev:server dev:web",
    "build:server": "yarn --cwd server build",
    "build:web": "yarn --cwd web build",
    "build": "run-s build:server build:web",
    "start": "yarn --cwd server start",
    "lint:web": "yarn --cwd web lint",
    "lint:web:fix": "yarn --cwd web lint:fix",
    "typecheck:web": "yarn --cwd web typecheck",
    "verify:web": "yarn --cwd web verify",
    "lint:server": "yarn --cwd server lint",
    "lint:server:fix": "yarn --cwd server lint:fix",
    "typecheck:server": "yarn --cwd server typecheck",
    "verify:server": "yarn --cwd server verify",
    "format:web": "yarn --cwd web format",
    "format:web:check": "yarn --cwd web format:check",
    "preview:web": "yarn --cwd web preview",
    "docs:dev": "yarn --cwd docs start",
    "docs:build": "yarn --cwd docs build",
    "docs:serve": "yarn --cwd docs serve",
    "docs:generate:api": "yarn --cwd docs generate:api",
    "docs:check": "yarn --cwd docs run check"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
